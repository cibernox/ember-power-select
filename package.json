  "description": "The extensible select component built for ember",
  "keywords": [
    "ember-addon",
    "select",
    "select2",
    "selectize",
    "dropdown",
    "ember-power"
  ],
  "license": "MIT",
  "author": {
    "name": "Miguel Camba",
    "email": "miguel.camba@gmail.com"
  },
