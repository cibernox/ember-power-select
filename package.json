{
  "name": "ember-power-select",
  "version": "6.0.1",
  "description": "The extensible select component built for ember",
  "keywords": [
    "ember-addon",
    "select",
    "select2",
    "selectize",
    "dropdown",
    "ember-power"
  ],
  "homepage": "http://www.ember-power-select.com",
  "repository": {
    "type": "git",
    "url": "http://github.com/cibernox/ember-power-select.git"
  },
  "license": "MIT",
  "author": {
    "name": "Miguel Camba",
    "email": "miguel.camba@gmail.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@embroider/util": "^1.0.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-assign-helper": "^0.4.0",
    "ember-basic-dropdown": "^6.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.0",
    "ember-cli-typescript": "^4.2.0",
    "ember-concurrency": ">=1.0.0 <3",
    "ember-concurrency-decorators": "^2.0.0",
    "ember-text-measurer": "^0.6.0",
    "ember-truth-helpers": "^3.0.0"
  },
  "devDependencies": {
    "@ember-decorators/component": "^6.1.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.8.1",
    "@embroider/test-setup": "^1.8.3",
    "@types/ember": "^3.16.0",
    "@types/ember-data": "^3.16.1",
    "@types/ember-qunit": "^3.4.14",
    "@types/ember__test-helpers": "^2.0.0",
    "@types/qunit": "^2.11.1",
    "@types/rsvp": "^4.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.2",
    "ember-cli": "~4.6.0",
    "ember-cli-blueprint-test-helpers": "^0.19.2",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-deprecation-workflow": "^2.1.0",
    "ember-cli-fastboot": "^3.2.0-beta.5",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^3.0.0-alpha.3",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-terser": "^4.0.0",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-code-example-component": "^0.4.0",
    "ember-code-snippet": "^2.4.0",
    "ember-data": "^3.28.9",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-fetch": "^8.1.0",
    "ember-href-to": "^5.0.0",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~3.28.6",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.10.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^11.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "loader.js": "^4.7.0",
    "memory-scroll": "^1.0.2",
    "miragejs": "^0.1.45",
    "mocha": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.2.1",
    "release-it-lerna-changelog": "^3.1.0",
    "sass": "^1.43.0",
    "testdouble": "^3.16.0",
    "typescript": "~4.4.0",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.9.0"
  }
}
