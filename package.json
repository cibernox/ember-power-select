{
  "name": "ember-power-select",
  "version": "4.0.0-beta.6",
  "description": "The extensible select component built for ember",
  "keywords": [
    "ember-addon",
    "select",
    "select2",
    "selectize",
    "dropdown",
    "ember-power"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/cibernox/ember-power-select.git"
  },
  "license": "MIT",
  "author": {
    "name": "Miguel Camba",
    "email": "miguel.camba@gmail.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "http://www.ember-power-select.com",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "nodetest": "$(npm bin)/mocha node-tests --recursive",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "files": [
    "addon/",
    "app/",
    "blueprints/",
    "addon-test-support/",
