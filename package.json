{
  "name": "ember-power-select",
  "version": "7.1.3",
  "description": "The extensible select component built for ember",
  "keywords": [
    "ember-addon",
    "select",
    "select2",
    "selectize",
    "dropdown",
    "ember-power"
  ],
  "homepage": "http://www.ember-power-select.com",
  "repository": {
    "type": "git",
    "url": "http://github.com/cibernox/ember-power-select.git"
  },
  "license": "MIT",
  "author": {
    "name": "Miguel Camba",
    "email": "miguel.camba@gmail.com"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "@embroider/util": "^1.11.0",
    "@ember/string": "^3.1.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-assign-helper": "^0.4.0",
    "ember-auto-import": "^2.6.3",
    "ember-basic-dropdown": "^7.2.2",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-typescript": "^5.0.0",
    "ember-concurrency": "^2.0.0 || ^3.0.0",
    "ember-text-measurer": "^0.6.0",
    "ember-truth-helpers": "^3.1.0 || ^4.0.0"
  },
  "devDependencies": {
    "@ember-decorators/component": "^6.1.1",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.0.3",
    "@embroider/test-setup": "^3.0.1",
    "@glint/template": "^1.0.2",
    "@types/ember": "^4.0.3",
    "@types/ember-data": "^4.4.10",
    "@types/qunit": "^2.19.5",
    "@types/rsvp": "^4.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.12.1",
    "ember-cli-blueprint-test-helpers": "^0.19.2",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^3.0.0-alpha.3",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-code-example-component": "^0.4.1",
    "ember-code-snippet": "git+https://git@github.com/ef4/ember-code-snippet.git#d054b697098ad52481c94a952ccf8d89ba1f25fe",
    "ember-data": "~4.12.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-href-to": "^5.0.0",
    "ember-load-initializers": "^2.1.2",
    "ember-prism": "^0.13.0",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^10.1.0",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.10.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "memory-scroll": "^1.0.2",
    "miragejs": "^0.1.47",
    "mocha": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.11.0",
    "release-it-lerna-changelog": "^5.0.0",
    "sass": "^1.63.3",
    "testdouble": "^3.18.0",
    "typescript": "~4.9.5",
    "webpack": "^5.86.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "overrides": {
    "ember-source": "$ember-source"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  },
  "volta": {
    "node": "16.19.0"
  }
}
