<input type="text" placeholder="Focus me and press TAB to focus the select" style="line-height: 2; width: 100%">
<br>
<br>
<PowerSelect
  @searchEnabled={{true}}
  @options={{this.diacritics}}
  @selected={{this.fellow}}
  @labelText="Name"
  @onChange={{fn (mut this.fellow)}} as |name|>
  {{name}}
</PowerSelect>